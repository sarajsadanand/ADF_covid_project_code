{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "saraj-covid-reporting-adf"
		},
		"ls_ablob_sarajcovidreportingsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_sarajcovidreportingsa'"
		},
		"ls_adls_sarajcovidreportingdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_sarajcovidreportingdl'"
		},
		"ls_adls_sarajcovidreportingdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sarajcovidreportingdl.dfs.core.windows.net/"
		},
		"ls_http_github_azure_data_factory_project_covid19_main_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/sarajsadanand/azure_data_factory_project_covid19/main/"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/27062347-708a-405a-a59f-a65b115422dc/resourceGroups/covid-reporting-rg/providers/Microsoft.Storage/storageAccounts/sarajcovidreportingsa"
		},
		"tr_ingest_hospital_admissions_data_properties_pl_ingest_github_ecdc_data_parameters_sourceRelativeURL": {
			"type": "string",
			"defaultValue": "ecdc_data/hospital_admissions.csv"
		},
		"tr_ingest_hospital_admissions_data_properties_pl_ingest_github_ecdc_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "saraj-covid-reporting-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_sarajcovidreportingsa_connectionString": {
						"value": "[parameters('ls_ablob_sarajcovidreportingsa_connectionString')]"
					},
					"ls_adls_sarajcovidreportingdl_accountKey": {
						"value": "[parameters('ls_adls_sarajcovidreportingdl_accountKey')]"
					},
					"ls_adls_sarajcovidreportingdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_sarajcovidreportingdl_properties_typeProperties_url')]"
					},
					"ls_http_github_azure_data_factory_project_covid19_main_properties_typeProperties_url": {
						"value": "[parameters('ls_http_github_azure_data_factory_project_covid19_main_properties_typeProperties_url')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					},
					"tr_ingest_hospital_admissions_data_properties_pl_ingest_github_ecdc_data_parameters_sourceRelativeURL": {
						"value": "[parameters('tr_ingest_hospital_admissions_data_properties_pl_ingest_github_ecdc_data_parameters_sourceRelativeURL')]"
					},
					"tr_ingest_hospital_admissions_data_properties_pl_ingest_github_ecdc_data_parameters_sinkFileName": {
						"value": "[parameters('tr_ingest_hospital_admissions_data_properties_pl_ingest_github_ecdc_data_parameters_sinkFileName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}